{
  "name": "@elizaos/plugin-github",
  "version": "0.0.6",
  "description": "GitHub integration plugin for ElizaOS",
  "main": "dist/index.js",
  "type": "commonjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "scripts": {
    "build": "tsup --format cjs,esm --dts",
    "test": "jest",
    "clean": "rm -rf dist",
    "dev": "tsup --format cjs,esm --dts --watch",
    "lint": "eslint ."
  },
  "dependencies": {
    "@elizaos/core": "workspace:*",
    "@octokit/rest": "^20.0.2",
    "@octokit/auth-token": "^4.0.0",
    "js-tiktoken": "^1.0.8",
    "proper-lockfile": "^4.1.2"
  },
  "devDependencies": {
    "@types/node": "^20.11.16",
    "@types/proper-lockfile": "^4.1.4",
    "typescript": "^5.0.0"
  }
}